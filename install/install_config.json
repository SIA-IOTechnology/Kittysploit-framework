{
  "framework": {
    "name": "KittySploit Framework",
    "version": "1.0.2",
    "description": "Advanced penetration testing framework",
    "author": "KittySploit Team",
    "license": "MIT"
  },
  "requirements": {
    "python": {
      "min_version": "3.8",
      "recommended_version": "3.9"
    },
    "packages": [
      "requests>=2.28.0",
      "colorama>=0.4.4",
      "cryptography>=3.4.8",
      "websockets>=10.0",
      "aiohttp>=3.8.0",
      "asyncio-mqtt>=0.11.0",
      "flask>=2.0.0",
      "flask-cors>=3.0.10",
      "paramiko>=2.9.0",
      "scapy>=2.4.5",
      "netaddr>=0.8.0",
      "psutil>=5.8.0",
      "pytest>=6.2.0",
      "pytest-asyncio>=0.18.0",
      "prompt_toolkit>=3.0.0"
      "pyngrok>=5.0.0",
      "bleak",
      "nava",
      "six",
      "docker",
      "sqlalchemy",
      "pymysql",
      "paramiko",
      "toml",
      "bcrypt>=4.0.0",
      "pyserial",
      "pychromecast"
    ],
    "optional_packages": [
      "selenium>=4.0.0",
      "beautifulsoup4>=4.10.0",
      "lxml>=4.6.0",
      "pillow>=8.3.0"
    ]
  },
  "installation": {
    "scripts": {
      "windows": {
        "installer": "install.bat",
        "powershell": "install.ps1",
        "start": "start_kittysploit.bat",
        "uninstall": "uninstall.bat"
      },
      "linux": {
        "installer": "install.sh",
        "start": "start_kittysploit.sh",
        "uninstall": "uninstall.sh"
      },
      "macos": {
        "installer": "install.sh",
        "start": "start_kittysploit.sh",
        "uninstall": "uninstall.sh"
      }
    },
    "files": {
      "core": [
        "kittysploit.py",
        "requirements.txt",
        "setup.py"
      ],
      "modules": [
        "modules/"
      ],
      "core_framework": [
        "core/"
      ],
      "interfaces": [
        "interfaces/"
      ]
    }
  },
  "features": {
    "browser_exploitation": true,
    "network_scanning": true,
    "payload_generation": true,
    "session_management": true,
    "modular_architecture": true,
    "web_interface": true,
    "api_support": true
  },
  "platforms": {
    "windows": {
      "supported": true,
      "min_version": "Windows 10",
      "recommended": "Windows 11"
    },
    "linux": {
      "supported": true,
      "min_version": "Ubuntu 18.04",
      "recommended": "Ubuntu 20.04+"
    },
    "macos": {
      "supported": true,
      "min_version": "macOS 10.14",
      "recommended": "macOS 12+"
    }
  }
}
